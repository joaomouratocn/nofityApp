<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<VBox alignment="TOP_CENTER" maxHeight="200.0" maxWidth="300.0" minHeight="200.0" minWidth="300.0" prefHeight="200.0"
      prefWidth="300.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.arthivia.notifyapp.controllers.NotificationController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true"/>
    <Label prefHeight="36.0" prefWidth="331.0" text="Notification title">
        <font>
          <Font size="25.0"/>
        </font>
    </Label>
    <Label prefHeight="59.0" prefWidth="327.0" text="Notification Message">
        <font>
          <Font size="15.0"/>
        </font>
    </Label>
    <Button mnemonicParsing="false" text="Confirmar"/>
</VBox>
